/**
 * 
 */
function login(){
	var username = document.getElementById("username").value;
	var password = document.getElementById("password").value;
	
	alert(username);
	alert(password);
	
	getData();
}

function getData(){
	fetch('https://coding-test.rootstack.net')
	.then((response) => {
	  return response.text(); // or .text() or .blob() ...
	})
	.then((text) => {
	  alert(text);
	})
	.catch((e) => {
	  alert(e.message);
	});
}